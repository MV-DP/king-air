<PropertyList>

    <path>c90.ac</path>

    <nasal>
        <load>
            var livery_update = aircraft.livery_update.new("Aircraft/king-air/Models/Liveries", 10);
        </load>

        <unload>
            livery_update.stop();
        </unload>
    </nasal>

