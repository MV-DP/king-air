   <model>
        <name>shadow</name>
        <path>Aircraft/king-air/Models/C90A/shadow.xml</path>
        <offsets>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0.05</z-m>
            <pitch-deg>0</pitch-deg>
        </offsets>
    </model>

    <animation>
        <type>select</type>
        <object-name>Fuselage.inner</object-name>
        <condition>
            <not>
                <property>sim/current-view/internal</property>
            </not>
        </condition>
    </animation>

    <model>
        <name>cabin</name>
        <path>Aircraft/king-air/Models/C90A/cabin.xml</path>
        <offsets>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
            <pitch-deg>0</pitch-deg>
        </offsets>
    </model>

    <model>
        <name>fdeck</name>
        <path>Aircraft/king-air/Models/C90A/fdeck.xml</path>
        <offsets>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
            <pitch-deg>0</pitch-deg>
        </offsets>
    </model>

    <animation>
        <type>range</type>
        <object-name>cabin</object-name>
        <object-name>fdeck</object-name>
        <min-m>0</min-m>
        <max-m>10</max-m>
    </animation>

<!-- Engines -->

    <animation>
        <type>select</type>
        <object-name>LHprop.disc</object-name>
        <condition>
            <and>
                <not>
                    <property>sim/current-view/internal</property>
                </not>
                <greater-than>
                    <property>engines/engine[0]/n1</property>
                    <value>50</value>
                </greater-than>
            </and>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>RHprop.disc</object-name>
        <condition>
            <and>
                <not>
                    <property>sim/current-view/internal</property>
                </not>
                <greater-than>
                    <property>engines/engine[1]/n1</property>
                    <value>50</value>
                </greater-than>
            </and>
        </condition>
    </animation>

