<?xml version="1.0"?>
<PropertyList>

    <path>fflowgauge.ac</path>

    <animation>
        <type>material</type>
        <object-name>PPHBezel</object-name>
        <emission>
            <red>0.01</red>
            <green>0.01</green>
            <blue>0.01</blue>
            <factor-prop>/controls/lighting/panel-norm</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>PPHFace</object-name>
        <object-name>PPH.needle</object-name>
        <emission>
            <red>0.035</red>
            <green>0.012</green>
            <blue>0.007</blue>
            <factor-prop>/systems/electrical/outputs/eng-lights</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>PPH.needle</object-name>
        <property>/engines/engine[0]/fuel-flow-gph</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>119.0</ind><dep>240.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0.0</z>
        </axis>
    </animation>

</PropertyList>