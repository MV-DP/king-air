<?xml version="1.0"?>
<PropertyList>

    <path>ittgauge.ac</path>

    <animation>
        <type>material</type>
        <object-name>ITTFace</object-name>
        <object-name>ITT.needle</object-name>
        <emission>
            <red>0.035</red>
            <green>0.012</green>
            <blue>0.007</blue>
            <factor-prop>systems/electrical/outputs/eng-lights</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>ITT.needle</object-name>
        <property>engines/engine[0]/torque-ftlb</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>5000.0</ind><dep>180.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0.0</z>
        </axis>
    </animation> 

</PropertyList>