<?xml version="1.0"?>
<PropertyList>

    <path>fuelqty.ac</path>

    <animation>
        <type>material</type>
        <object-name>Fuelface</object-name>
        <object-name>FQneedle.1</object-name>
        <object-name>FQneedle.2</object-name>
        <object-name>FQneedle.3</object-name>
        <object-name>FQneedle.4</object-name>
        <emission>
            <red>0.035</red>
            <green>0.012</green>
            <blue>0.007</blue>
            <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>FQneedle.1</object-name>
        <property>/consumables/fuel/tank/level-lbs</property>
        <interpolation>
            <entry>
                <ind>0.0</ind><dep>0.0</dep>
            </entry>
            <entry>
                <ind>2000.0</ind><dep>240.0</dep>
            </entry>
        </interpolation>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0.0</x-m>
            <y-m>-0.060</y-m>
            <z-m>-0.003</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>FQneedle.3</object-name>
        <property>/consumables/fuel/tank[2]/level-lbs</property>
        <interpolation>
            <entry>
                <ind>0.0</ind><dep>0.0</dep>
            </entry>
            <entry>
                <ind>2000.0</ind><dep>240.0</dep>
            </entry>
        </interpolation>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0.0</x-m>
            <y-m>-0.060</y-m>
            <z-m>-0.003</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>FQneedle.2</object-name>
        <property>/consumables/fuel/tank[1]/level-lbs</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>2000.0</ind><dep>240.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0.050</y-m>
            <z-m>-0.003</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>FQneedle.4</object-name>
        <property>/consumables/fuel/tank[3]/level-lbs</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>2000.0</ind><dep>240.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0.050</y-m>
            <z-m>-0.003</z-m>
        </center>
    </animation>

    <animation>
        <type>select</type>
        <object-name>FQneedle.1</object-name>
        <object-name>FQneedle.2</object-name>
        <condition>
            <property>controls/fuel/gauge-switch</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>FQneedle.3</object-name>
        <object-name>FQneedle.4</object-name>
        <condition>
            <not>
                <property>controls/fuel/gauge-switch</property>
            </not>
        </condition>
    </animation>

</PropertyList>